pip install
import numpy as np

a = np.array([1,2,3])
print(a)
[1 2 3]


b = np.array([[4,5,6],[7,8,9]])
print (b)
[[4 5 6]
 [7 8 9]]


# TO GET DIMENSION USE .ndim
a.ndim
1


# TO GET SHAPE USE .shape
b.shape
(2, 3)


# TO GET TYPE .dtype
k = np.array([[5,6,7],[4,2,5]] , dtype = 'int')
k.dtype
dtype('int64')


# TO GET SIZE USE .itemsize
k.itemsize
8


# TO GET TOTAL SIZE .nbytes
k.nbytes
48
